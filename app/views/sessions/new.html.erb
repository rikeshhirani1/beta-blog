<h1>Log in</h1>

<%= form_for :session, url: login_path do |f| %>
  <p>
    <%= f.label :email %><br/>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %><br/>
    <%= f.password_field :password, autocomplete: "off" %>
  </p>
  <p>
    <%= f.submit "Log in" %>
  </p>

<% end %>